{
    "coverage": {
        "US Census": {
            "geoid": "06093",
            "name": "Siskiyou County",
            "state": "California"
        },
        "country": "us",
        "state": "ca",
        "county": "Siskiyou"
    },
    "schema": 2,
    "layers": {
        "addresses": [
            {
                "name": "county",
                "note": "Obtained via FOIA request - ingalls - 2017/11, updated by justin - 2018/02",
                "data": "https://www.co.siskiyou.ca.us/sites/default/files/fileattachments/community_development/page/2461/siskiyouparcelsnov2017.zip",
                "protocol": "http",
                "conform": {
                    "format": "csv",
                    "lat": "POINT_Y",
                    "lon": "POINT_X",
                    "number": {
                        "function": "prefixed_number",
                        "field": "ADDR_1"
                    },
                    "street": {
                        "function": "postfixed_street",
                        "field": "ADDR_1",
                        "may_contain_units": true
                    },
                    "unit": {
                        "function": "postfixed_unit",
                        "field": "ADDR_1"
                    },
                    "city": {
                        "function": "regexp",
                        "field": "ADDR_2",
                        "pattern": "(.*) CA .*$",
                        "replace": "$1"
                    },
                    "postcode": {
                        "function": "regexp",
                        "field": "ADDR_2",
                        "pattern": ".*([0-9]{5})$",
                        "replace": "$1"
                    }
                }
            }
        ]
    }
}
